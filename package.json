{
  "name": "scoring-api",
  "version": "1.1.0",
  "description": "Boilerplate repository to setup swagger 3.0",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0",
    "npm": "6.14.10"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "test": "jest",
    "develop": "NODE_ENV=DEVELOPMENT nodemon --use_strict index.js",
    "start": "node --use_strict index.js",
    "run:docker": "docker-compose up --build app",
    "mongo-local": "bash local.sh",
    "local": "nodemon --use-strict src/.",
    "dev": "npm run develop",
    "release": "standard-version"
  },
  "author": "Nikhil Vijayan",
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "aws-sdk": "2.785.0",
    "body-parser": "^1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "express-openapi-validator": "4.10.5",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.10.11",
    "pino": "6.7.0",
    "trash-cli": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "husky": "^4.2.3",
    "jest": "26.6.3",
    "nodemon": "2.0.3",
    "standard-version": "^9.1.0",
    "supertest": "6.1.3"
  }
}