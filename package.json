{
  "name": "leaderboard-api",
  "version": "1.1.0",
  "description": "Boilerplate repository to setup swagger 3.0",
  "main": "index.js",
  "engines": {
    "node": ">=12.0.0",
    "npm": "6.14.10"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=/test/unit",
    "develop": "NODE_ENV=DEVELOPMENT nodemon --use_strict index.js",
    "start": "node --use_strict index.js",
    "app:docker": "docker-compose up --build app",
    "test:docker": "bash test.sh",
    "mongo-local": "bash local.sh",
    "local": "nodemon --use-strict index.js",
    "dev": "npm run develop",
    "release": "standard-version"
  },
  "author": "Nikhil Vijayan",
  "license": "MIT",
  "dependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "aws-sdk": "2.785.0",
    "body-parser": "^1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "express-openapi-validator": "4.10.5",
    "express-rate-limit": "5.2.6",
    "jsonwebtoken": "8.5.1",
    "mongoose": "^5.12.1",
    "pino": "6.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@subgeniuscorp/eslint-config-node": "^1.3.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.2.3",
    "jest": "26.6.3",
    "jwt-decode": "^3.1.2",
    "nodemon": "2.0.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "supertest": "6.1.3"
  }
}
